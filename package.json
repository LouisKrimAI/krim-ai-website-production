{
  "name": "krim-v3-future-web",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "sideEffects": [
    "src/components/ParticleUniverse.tsx",
    "src/shims/react-batchconfig-shim.ts",
    "src/shims/**/*.ts",
    "src/**/*polyfill*",
    "src/**/*shim*"
  ],
  "engines": {
    "node": ">=20.16.0",
    "npm": ">=10"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:analyze": "npm run build && npx vite-bundle-analyzer dist/stats.html",
    "build:performance": "VITE_PERFORMANCE_MODE=true npm run build",
    "preview": "vite preview",
    "preview:https": "vite preview --https",
    "preview:serve": "vite preview --port 4173",
    "performance:audit": "npm run build && npx lighthouse http://localhost:4173 --output json --output-path ./performance-audit.json",
    "performance:budget": "npm run build && node scripts/performance-budget-check.js",
    "cache:clear": "rm -rf dist && rm -rf node_modules/.vite",
    "analyze:bundle": "npx vite-bundle-analyzer",
    "clean": "rimraf .next dist build .turbo coverage .lighthouse artifacts || true",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint . --max-warnings=0",
    "check:console": "node scripts/check-console.cjs http://localhost:4173",
    "gate:forbid": "node scripts/forbid-patterns.cjs",
    "gate:zero": "npm run clean && npm run typecheck && npm run lint && npm run build && start-server-and-test preview:serve http-get://localhost:4173 \"npm run check:console\"",
    "lh": "lhci autorun",
    "test:cta": "playwright test",
    "test:cta:preview": "start-server-and-test preview:serve http-get://localhost:4173 \"playwright test\"",
    "test:install": "playwright install --with-deps chromium",
    "gate:all": "npm run gate:forbid && npm run gate:zero && npm run lh && npm run test:cta:preview && node scripts/compare-content.cjs",
    "assert:no3d:initial": "node scripts/assert-no-r3f-on-initial.cjs",
    "assert:bundle:clean": "node scripts/assert-entry-bundle-clean.cjs"
  },
  "dependencies": {
    "@gsap/react": "^2.1.2",
    "@mendable/firecrawl-js": "^4.3.4",
    "@modelcontextprotocol/sdk": "^1.17.5",
    "@modelcontextprotocol/server-filesystem": "^2025.8.21",
    "@modelcontextprotocol/server-memory": "^2025.8.4",
    "@modelcontextprotocol/server-puppeteer": "^2025.5.12",
    "@phosphor-icons/react": "^2.1.10",
    "@react-three/drei": "9.88.17",
    "@react-three/fiber": "8.15.16",
    "@react-three/postprocessing": "^2.16.0",
    "@rive-app/canvas": "^2.31.5",
    "@rive-app/react-canvas": "^4.23.3",
    "@supabase/mcp-server-supabase": "^0.5.3",
    "@supabase/supabase-js": "^2.57.3",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "clsx": "^2.1.1",
    "coderabbitai-mcp": "^1.1.1",
    "express": "^5.2.1",
    "framer-motion": "^11.3.23",
    "gsap": "^3.13.0",
    "lenis": "^1.3.11",
    "lottie-react": "^2.4.1",
    "lucide-react": "^0.454.0",
    "motion": "^12.23.12",
    "playwright": "1.55.0",
    "postprocessing": "^6.37.7",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-router-dom": "^6.26.2",
    "resend": "^6.1.2",
    "three": "^0.160.0",
    "use-sync-external-store": "^1.5.0",
    "vanilla-tilt": "^1.8.1",
    "vercel-mcp": "^0.0.7",
    "web-vitals": "^5.1.0"
  },
  "devDependencies": {
    "@lhci/cli": "^0.13.0",
    "@playwright/mcp": "^0.0.41",
    "@playwright/test": "1.55.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/node": "^22.5.5",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/three": "^0.164.1",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^5.0.4",
    "autoprefixer": "^10.4.21",
    "chalk": "^5.6.2",
    "eslint": "^8.57.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "fast-glob": "^3.3.2",
    "madge": "^8.0.0",
    "playwright": "1.55.0",
    "postcss": "^8.4.47",
    "puppeteer": "^24.34.0",
    "rimraf": "^5.0.5",
    "rollup-plugin-visualizer": "^6.0.3",
    "sharp": "^0.34.5",
    "start-server-and-test": "^2.0.3",
    "tailwindcss": "^3.4.10",
    "typescript": "~5.6.2",
    "vite": "^5.4.20",
    "vite-plugin-pwa": "^1.0.3"
  }
}
